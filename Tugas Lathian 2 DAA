5. import math
   def volume_tabung(r,t):
  return math.pi*r**2*t

  jari_jari=3
  tinggi=5
  print("jari-jari=",jari_jari,"\ntinggi=tinggi,"\nluas=",volume_tabung(jari_jari,tinggi)

6. import math
   def volime_kerucut(d,t):
   r=d/2
   return 1/3*math.pi*r**2*t

   diameter=5
   tinggi=4
   print("diameter="diameter,"\ntinggi="tinggi,"\volume=",volume_kerucut(diameter,tinggi)



            
