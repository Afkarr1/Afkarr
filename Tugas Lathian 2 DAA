1. def kpk(a,b):
    if a>b:
      terbesar=a
    else:
      terbesar=b

    while true:
      if terbesar%a== and terbesar%b==0:
         hasil=terbsesar
         break
         terbesar+=1
    return hasil

a = 3
b = 4
print("kpk dari"a"dan",b,"adalah",kpk(a,b))


2. def menukar(a,y):
     piring=[a,y,""]
     for in range(len(piring)):
     piring[2]=a
     piring[0]=b
     piring[1]=a
     piring[2]=""
  print("piring,i+1,"=",str(piring[1]))

